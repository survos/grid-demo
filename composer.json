{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/core": "3.1.7",
        "doctrine/annotations": "^1.14.3",
        "doctrine/doctrine-bundle": "^2.9.1",
        "doctrine/doctrine-migrations-bundle": "^3.2.2",
        "doctrine/orm": "^2.15.1",
        "friendsofsymfony/jsrouting-bundle": "^3.2.1",
        "nelmio/cors-bundle": "^2.3.1",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.20.4",
        "survos/api-grid-bundle": "^1.4.41",
        "survos/bootstrap-bundle": "^1.4.41",
        "survos/command-bundle": "^1.4",
        "survos/core-bundle": "^1.4.41",
        "survos/crawler-bundle": "^1.4",
        "survos/grid-bundle": "^1.4.41",
        "survos/inspection-bundle": "^1.4.41",
        "symfony/asset": "^6.2.7",
        "symfony/console": "^6.2.10",
        "symfony/doctrine-messenger": "^6.2.10",
        "symfony/dotenv": "^6.2.8",
        "symfony/expression-language": "^6.2.7",
        "symfony/flex": "^2.2.5",
        "symfony/form": "^6.2.10",
        "symfony/framework-bundle": "^6.2.10",
        "symfony/http-client": "^6.2.10",
        "symfony/intl": "^6.2.10",
        "symfony/mailer": "^6.2.8",
        "symfony/mime": "^6.2.10",
        "symfony/monolog-bundle": "^3.8",
        "symfony/notifier": "^6.2.8",
        "symfony/process": "^6.2.10",
        "symfony/property-access": "^6.2.8",
        "symfony/property-info": "^6.2.10",
        "symfony/proxy-manager-bridge": "^6.2.7",
        "symfony/runtime": "^6.2.8",
        "symfony/security-bundle": "^6.2.10",
        "symfony/serializer": "^6.2.10",
        "symfony/string": "^6.2.8",
        "symfony/translation": "^6.2.8",
        "symfony/twig-bundle": "^6.2.7",
        "symfony/validator": "^6.2.10",
        "symfony/web-link": "^6.2.7",
        "symfony/webpack-encore-bundle": "^1.16.1",
        "symfony/workflow": "^6.2.7",
        "symfony/yaml": "^6.2.10",
        "twig/extra-bundle": "^3.6",
        "twig/twig": "^3.6"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "php-http/discovery": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "fos:js-routing:dump --format=json --target=public/js/fos_js_routes.json": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^6.2"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4.4",
        "phpunit/phpunit": "^9.6.7",
        "survos/maker-bundle": "^1.4.36",
        "symfony/browser-kit": "^6.2.7",
        "symfony/css-selector": "^6.2.7",
        "symfony/debug-bundle": "^6.2.7",
        "symfony/maker-bundle": "^1.48",
        "symfony/phpunit-bridge": "^6.2.10",
        "symfony/stopwatch": "^6.2.7",
        "symfony/web-profiler-bundle": "^6.2.10",
        "symplify/easy-coding-standard": "^11.3.4"
    },
    "repositories": {
    }
}
